# HauserResults API Configuration
API_BASE_URL=https://hauserresults.hu
API_ERROR_CODE=askforit
USER_AGENT=something

# HTTP Configuration  
REQUEST_TIMEOUT_SECONDS=30

# Data Storage
CSV_OUTPUT_DIR=./output
CSV_DELIMITER=,

# Export Configuration
CSV_EXPORT_ENABLED=true
EXCEL_EXPORT_ENABLED=true
EXCEL_FILENAME=rally_data.xlsx

# Racing Number and Stage Settings
RACING_NUMBER_TAB=Settings
RACING_NUMBER_RANGE=A1:D3
RACING_SPEED_FILENAME=racing_speed

# HTTP Server Configuration
HTTP_SERVER_HOST=localhost
HTTP_SERVER_PORT=8000

# Logging
LOG_LEVEL=DEBUG
LOG_FILE=rally_data.log
